<mvc:View
    controllerName="app.home.controller.home"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns:l="sap.ui.layout"
    xmlns:tnt="sap.tnt"
>
    <Page id="_IDGenPage">
        <content>
            <GenericTile
                id="ID_config"
                class="sapUiMediumMargin"
                header="Dealer"
                subheader="Create Sales Order"
                press="onDealerClick"
                visible="false"
            >
                <TileContent id="_IDGenTileContent1">
                    <ImageContent
                        id="_IDGenImageContent"
                        src="sap-icon://sales-order"
                    />
                </TileContent>
            </GenericTile>
            <GenericTile
                id="boxQr"
                class="sapUiMediumMargin"
                header="Batch Number"
                subheader="Qr genration for Boxes"
                press="onBatchClick"
                visible="false"
            >
                <TileContent id="_IDGenTileContent2">
                    <ImageContent
                        id="_IDGenImageContent2"
                        src="sap-icon://qr-code"
                    />
                </TileContent>
            </GenericTile>

             <GenericTile
                id="boxQr1"
                class="sapUiMediumMargin"
                header="Dealer screen"
                subheader="Distributed Screen"
                press="onsplitDealerclick"
            >
                <TileContent id="_IDGenTileContent22">
                    <ImageContent
                        id="_IDGenImageContent22"
                        src="sap-icon://qr-code"
                    />
                </TileContent>
            </GenericTile>

             <GenericTile
                id="boxQr1e"
                class="sapUiMediumMargin"
                header="Inventory Dashboard"
                subheader="Dashboard Screen"
                press="onclickDashboard"
                
            >
                <TileContent id="_IDGenTileContent22d">
                    <ImageContent
                        id="_IDGenImageContent2d2"
                        src="sap-icon://qr-code"
                    />
                </TileContent>
            </GenericTile>
             <GenericTile
                id="_IDGenGenericTile3"
                class="sapUiMediumMargin"
                header="Create Order"
                press="onPressCreateOrderTile"
            >
                <TileContent id="_IDGenTileContent3">
                    <ImageContent
                        id="_IDGenImageContent3"
                        src="sap-icon://decision"
                    />
                </TileContent>
            </GenericTile>
        </content>
    </Page>
</mvc:View>
